<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.hikinghelper.cody.hikinghelper.Home"
    tools:showIn="@layout/app_bar_home">

    <Button
        android:id="@+id/btnEvents"
        android:layout_width="162dp"
        android:layout_height="49dp"
        android:layout_marginBottom="251dp"
        android:text="Events"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/btnUser"
        android:layout_width="162dp"
        android:layout_height="47dp"
        android:layout_marginBottom="31dp"
        android:layout_marginTop="30dp"
        android:text="Your Profile"
        app:layout_constraintBottom_toTopOf="@+id/btnEvents"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSearch"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="241dp" />

    <Button
        android:id="@+id/btnSearch"
        android:layout_width="162dp"
        android:layout_height="47dp"
        android:text="Mountain Search"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="1dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/btnGuide"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnUser"
        android:layout_marginLeft="1dp"
        tools:layout_editor_absoluteY="156dp" />

    <Button
        android:id="@+id/btnGuide"
        android:layout_width="162dp"
        android:layout_height="47dp"
        android:layout_marginTop="78dp"
        android:text="Guide"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="81dp" />
</android.support.constraint.ConstraintLayout>
